{
    "name": "bnp-lohan",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "db:reset": "npx supabase db reset",
        "db:seed": "npx tsx src/scripts/seed.ts",
        "db:types": "mkdir -p src/types && npx supabase gen types typescript --local > src/types/supabase.ts",
        "db:refresh": "npx supabase stop --no-backup && npx supabase start && npm run db:seed && npm run db:types",
        "format": "npx @biomejs/biome check --write .",
        "build": "next build",
        "start": "next start",
        "initialize": "npm install && npx supabase init && npx supabase start && npm run db:types && npm run db:seed"
    },
    "dependencies": {
        "@base-ui/react": "^1.0.0",
        "@supabase/ssr": "^0.8.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.3",
        "embla-carousel-react": "^8.6.0",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.562.0",
        "next": "16.1.1",
        "next-themes": "^0.4.6",
        "react": "19.2.3",
        "react-day-picker": "^9.13.0",
        "react-dom": "19.2.3",
        "react-resizable-panels": "^4.0.15",
        "recharts": "^2.15.4",
        "shadcn": "^3.6.2",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.4.0",
        "tw-animate-css": "^1.4.0",
        "vaul": "^1.1.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.1.0",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "babel-plugin-react-compiler": "1.0.0",
        "eslint": "^9",
        "eslint-config-next": "16.1.1",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
